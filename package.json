{
  "name": "demo-admin",
  "version": "0.1.0",
  "private": false,
  "description": "Demo Admin",
  "type": "module",
  "scripts": {
    "dev": "npm run schema && vite",
    "dev:mock": "npm run schema && VITE_MOCK_MODE='true' vite",
    "build": "npm run schema && tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint -c .eslintrc --ext .js,.jsx,.ts,.tsx . --fix",
    "plop": "plop --plopfile ./.plop/plop.config.cjs",
    "precommit": "npm run lint:fix & npm run build",
    "preview": "npm run build && vite preview",
    "schema": "node scripts/generateSchema.cjs --source=./src/types/validation.ts --output=./src/validation/schema.json"
  },
  "author": "Ann Bystrova <ann.bystrova96@mail.ru>",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^19.0.5",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-basic-ssl": "^1.2.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-react": "^13.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-unicorn": "^56.0.1",
    "plop": "^4.0.1",
    "prettier": "^3.4.2",
    "sass": "^1.83.1",
    "ts-json-schema-generator": "^2.3.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1",
    "vite": "^6.0.7",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.3.1",
    "@mui/material": "^6.3.1",
    "@mui/x-data-grid": "^7.24.0",
    "@mui/x-date-pickers": "^7.23.6",
    "@telegram-auth/react": "^1.0.4",
    "ajv": "^8.17.1",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "i18next": "^24.2.1",
    "library-react-hooks": "^0.23.0-alpha.29",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.0",
    "react-router-dom": "^7.1.1",
    "swiper": "^11.2.1"
  }
}
